{
    "name": "karelwintersky/arris.router",
    "description": "Karel Wintersky's Application µFramework - AppRouter class",
    "type": "library",
    "license": "MIT",
    "keywords": [ "routing" ],
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "Arris\\": "src/",
            "Arris\\AppRouter\\": "src/AppRouter"
        },
        "files": [
            "src/AppRouter/FastRoute/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Karel Wintersky",
            "email": "karel.wintersky@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 || 8.*",
        "psr/log": "^1 | ^2 | ^3"
    },
    "require-dev": {
        "karelwintersky/arris": "*"
    },
    "suggest": {
        "karelwintersky/arris": "Arris µFramework core",
        "karelwintersky/arris.logger": "Arris µFramework AppLogger",
        "karelwintersky/arris.helpers": "Arris µFramework helpers"
    }
}
